{
  "multipart": [
    {
      "apply": {
        "model": "chaosstorage:block/cable_core"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension",
        "x": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "chaosstorage:block/cable_extension",
        "x": 90
      }
    },
    {
      "when": {
        "direction": "north"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage"
      }
    },
    {
      "when": {
        "direction": "east"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage",
        "y": 90
      }
    },
    {
      "when": {
        "direction": "south"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage",
        "x": 180
      }
    },
    {
      "when": {
        "direction": "west"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage",
        "y": 270
      }
    },
    {
      "when": {
        "direction": "up"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage",
        "x": 270
      }
    },
    {
      "when": {
        "direction": "down"
      },
      "apply": {
        "model": "chaosstorage:block/external_storage",
        "x": 90
      }
    }
  ]
}